{
    "name": "vankosoft/vs_kubernetes",
    "version": "0.1.0",
    "author": "Ivan I. Atanasov",
    "summary": "Installs, configures, and manages a kubernetes cluster.",
    "license": "Apache-2.0",
    "source": "git://github.com/iatanasov77/puppet-kubernetes.git",
